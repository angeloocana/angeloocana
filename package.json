{
  "name": "blog",
  "description": "'My blog'",
  "version": "0.0.1",
  "author": "Angelo Ocana <angeloocana@gmail.com>",
  "scripts": {
    "prestart": "npm run lint",
    "start": "npm run develop",
    "lint": "./node_modules/.bin/eslint --fix --ext .js,.jsx --ignore-pattern public .",
    "build": "rm -rf public/*.html && gatsby build",
    "predeploy": "npm run lint",
    "deploy": "npm run build && surge public/ --domain angeloocana.surge.sh",
    "develop": "gatsby develop",
    "test": "jest",
    "test:watch": "jest --watch",
    "serve": "npm run build && gatsby serve"
  },
  "dependencies": {
    "babel-plugin-lodash": "^3.2.11",
    "bluebird": "^3.4.7",
    "es6-object-assign": "^1.0.3",
    "gatsby": "^1.0.4",
    "gatsby-link": "^1.3.2",
    "gatsby-plugin-google-analytics": "^1.0.1",
    "gatsby-plugin-manifest": "^1.0.1",
    "gatsby-plugin-offline": "^1.0.1",
    "gatsby-plugin-preact": "^1.0.1",
    "gatsby-plugin-react-helmet": "^1.0.1",
    "gatsby-plugin-sharp": "^1.3.0",
    "gatsby-remark-copy-linked-files": "^1.3.0",
    "gatsby-remark-prismjs": "^1.0.1",
    "gatsby-remark-responsive-iframe": "^1.3.0",
    "gatsby-remark-responsive-image": "next",
    "gatsby-remark-smartypants": "^1.0.1",
    "gatsby-source-filesystem": "^1.0.1",
    "gatsby-transformer-remark": "^1.0.1",
    "gatsby-transformer-sharp": "^1.0.1",
    "lodash": "^4.17.4",
    "lodash-webpack-plugin": "^0.11.2",
    "prettier": "^1.5.2",
    "proptypes": "^1.1.0",
    "react-gravatar": "^2.6.3",
    "react-icons": "^2.2.5",
    "styled-components": "^2.1.1"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "eslint": "^4.3.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.1.0",
    "eslint-plugin-standard": "^3.0.1",
    "jest": "^20.0.4",
    "ptz-assert": "^1.6.8",
    "ramda": "^0.24.1",
    "surge": "^0.19.0"
